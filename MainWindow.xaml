<Window x:Class="ideabuilder_status.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ideabuilder_status"
        mc:Ignorable="d"
        Title="Dremel IdeaBuilder Status" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnRefresh" Content="Refresh" HorizontalAlignment="Left" Margin="562,95,0,0" VerticalAlignment="Top" Click="btnRefresh_Click"/>
        <DataGrid x:Name="dgSettings" d:ItemsSource="{Binding}" HorizontalAlignment="Left" Width="392" />

    </Grid>
</Window>
